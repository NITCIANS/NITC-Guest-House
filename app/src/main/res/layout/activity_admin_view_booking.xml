                android:id="@+id/T10"
                android:layout_height="match_parent" />
        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:text="" />

            <TextView
                android:layout_width="225dp"
                android:layout_height="match_parent"
                android:text="" />

